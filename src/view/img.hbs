<header>
    {{> nav}}
</header>

<main>

    <section class="container m-5">
        <div class="row">
            <div class="col-12">
                <div class="alert alert-primary" role="alert">
                    {{url}}
                </div>
            </div>
        </div>
        <div class="row">
            {{#each data}}
            <div class="col-4 mb-4">
                <div class="card">
                    <img src="{{this}}" class="card-img-top" alt="...">
                    <div class="card-body">
                        <p class="card-header"><strong>ID:</strong> {{this}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </section>

</main>